{
    "manifest_version": 3,
    "name": "Outliner",
    "version": "1.0",
    "description": "A simple html element outliner",
    "author":{
        "email": "cj.ceps@gmail.com"
    },
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
          "matches": ["https://*/*", "http://*/*"], 
          "js": ["content.js"]
    }],
    "background": {
        "service_worker": "./background.js"
    }, 
    "permissions": [
        "scripting", "storage", "tabs"
    ]
  }